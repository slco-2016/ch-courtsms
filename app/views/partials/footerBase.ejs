<script type="text/javascript">
  // Place user object as global
  // user is either user object or null
  // keeping the if operations just to be explicit
  <% if (user) { %>
    var SESSION_USER = <%- JSON.stringify(user) %>;
  <% } else { %>
    var SESSION_USER = null;
  <% } %>
</script>
